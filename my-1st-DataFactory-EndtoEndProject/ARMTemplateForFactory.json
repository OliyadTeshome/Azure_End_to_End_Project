		"AzureDataLakeStorage_for_metadata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_for_metadata'"
		},
		"StorageDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'StorageDataLake'"
		},
		"AzureDataLakeStorage_for_metadata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://my1stendtoendproject.dfs.core.windows.net/"
		},
		"HttpLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"StorageDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://my1stendtoendproject.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
